#include "main.h"

/**
 * more_numbers - Prints 0 to 9, 10 times
 *
 * Return: Always 0 (Success)
 */

void more_numbers(void)
{
	int i = 0;
	char j;

	while (i < 10)
	{
		for (j = '0'; j <= '14';, j++)
		{
			if (j >= '10')
			{
				_putchar((j / 10) + '0');
			}
			_putchar((j % 10) + '0');
			_putchar('\n');
		}
		i++;
	}
}
